// RUN: xdsl-opt %s | xdsl-opt | filecheck %s


module() {
  func.func() ["sym_name" = "float_check", "function_type" = !fun<[],[]>, "sym_visibility" = "private"] {
    ^0(%0 : !float_type<[!ft32]>):
    func.return()
  }
}

